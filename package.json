{
  "name": "@mohism/cli-wrapper",
  "version": "1.3.22",
  "description": "以插件的方式创建你的命令行工具",
  "bin": {
    "init-wrapper": "./dist/bin/init.js",
    "gen-cmd": "./dist/bin/gen.js",
    "debug-cmd": "./dist/bin/debug.js",
    "cli-complete": "./dist/bin/cli-complete.js"
  },
  "scripts": {
    "build": "npx eslint src/**/*.ts --fix && npx tsc",
    "lint": "npx eslint src/**/*.ts --fix",
    "postinstall": "echo 'Run  \"npx init-wrapper\"'",
    "start": "npx ts-node ./src/bin/index.ts",
    "test": "npx mocha --require ts-node/register **/*.spec.ts",
    "cover": "npx nyc mocha --require ts-node/register **/*.spec.ts"
  },
  "keywords": [
    "mohism",
    "moism",
    "command"
  ],
  "author": "lanhao",
  "license": "ISC",
  "devDependencies": {
    "@types/colors": "^1.2.1",
    "@types/inquirer": "^6.5.0",
    "@types/node": "^12.7.12",
    "@typescript-eslint/eslint-plugin": "^2.5.0",
    "@typescript-eslint/parser": "^2.5.0",
    "chai": "^4.2.0",
    "dts-gen": "^0.5.8",
    "eslint": "^6.5.1",
    "mocha": "^6.2.2",
    "nodemon": "^1.19.4",
    "nyc": "^14.1.1",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.4"
  },
  "dependencies": {
    "@mohism/utils": "^1.0.1",
    "@types/shelljs": "^0.8.5",
    "@types/yargs": "^13.0.3",
    "colors": "^1.4.0",
    "dayjs": "^1.8.16",
    "inquirer": "^7.0.0",
    "shelljs": "^0.8.3",
    "yargs": "^14.2.0"
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mohism-framework/cli-wrapper.git"
  },
  "bugs": {
    "url": "https://github.com/mohism-framework/cli-wrapper/issues"
  },
  "homepage": "https://github.com/mohism-framework/cli-wrapper#readme"
}